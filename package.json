{
  "name": "devnetwork",
  "version": "1.0.0",
  "description": "Social network for developers",
  "main": "server.js",
  "engines": {
    "npm": "6.5.0",
    "node": "10.6.0"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Michael Womack",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.8.4",
    "react-scripts": "^2.1.5",
    "validator": "^10.11.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
